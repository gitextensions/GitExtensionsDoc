#: ..\z_appendix.rst:2
msgid "Appendix"
msgstr "Anhang"

#: ..\z_appendix.rst:5
msgid "Git Cheat Sheet"
msgstr "Git Spickzettel"

#: ..\z_appendix.rst:8
msgid "Action"
msgstr "Aktion"

#: ..\z_appendix.rst:8
msgid "Command"
msgstr "Befehl"

#: ..\z_appendix.rst:10
msgid "Create new repository"
msgstr "Neues Repository erzeugen"

#: ..\z_appendix.rst:10
msgid "`$ git init`"
msgstr "`$ git init`"

#: ..\z_appendix.rst:12
msgid "Create shared repository"
msgstr "Geteiltes Repository erzeugen"

#: ..\z_appendix.rst:12
msgid "`$ git init –-bare –-shared=all`"
msgstr "`$ git init –-bare –-shared=all`"

#: ..\z_appendix.rst:14
msgid "Clone repository"
msgstr "Repository klonen"

#: ..\z_appendix.rst:14
msgid "`$ git clone c:/demo1 c:/demo2`"
msgstr "`$ git clone c:/demo1 c:/demo2`"

#: ..\z_appendix.rst:16
msgid "Checkout branch"
msgstr "Branch auschecken"

#: ..\z_appendix.rst:16
msgid "`$ git checkout <name>`"
msgstr "`$ git checkout <name>`"

#: ..\z_appendix.rst:18
msgid "Create branch"
msgstr "Branch erzeugen"

#: ..\z_appendix.rst:18
msgid "`$ git branch <name>`"
msgstr "`$ git branch <name>`"

#: ..\z_appendix.rst:20
msgid "Delete branch"
msgstr "Branch löschen"

#: ..\z_appendix.rst:20
msgid "`$ git branch -d <name>`"
msgstr "`$ git branch -d <name>`"

#: ..\z_appendix.rst:22
msgid "Merge branch (from the branch to merge into):"
msgstr "Branch mergen (von dem Branch in den gemergt werden soll):"

#: ..\z_appendix.rst:22
msgid "`$ git merge PDC`"
msgstr "`$ git merge PDC`"

#: ..\z_appendix.rst:24 ..\z_appendix.rst:41 ..\z_appendix.rst:50
msgid "Solve conflicts (add --tool=kdiff3 if no mergetool is specified)"
msgstr "Konflikte lösen (--tool=kfiff3 wenn kein mergetool angegeben wurde)"

#: ..\z_appendix.rst:24
msgid "`$ git mergetool` `$ git commit`"
msgstr "`$ git mergetool` `$ git commit`"

#: ..\z_appendix.rst:27
msgid "Create tag"
msgstr "Tag erstellen"

#: ..\z_appendix.rst:27
msgid "`$ git tag <name>`"
msgstr "`$ git tag <name>`"

#: ..\z_appendix.rst:29
msgid "Add files/changes (. for all files)"
msgstr "Dateien/Änderungen hinzufügen (. für alle Dateien)"

#: ..\z_appendix.rst:29
msgid "`$ git add .`"
msgstr "`$ git add .`"

#: ..\z_appendix.rst:31
msgid "Commit added files/changes (--amend to amend to last commit)"
msgstr "Vorgemerkte Dateien/Änderungen comitten (--amend, um den vorigen Commit zu ändern)"

#: ..\z_appendix.rst:31
msgid "`$ git commit –m “Enter commit message”`"
msgstr "`$ git commit –m “Commit Nachricht eingeben”`"

#: ..\z_appendix.rst:33
msgid "Discard changes"
msgstr "Änderungen verwerfen"

#: ..\z_appendix.rst:33
msgid "`$ git reset --hard`"
msgstr "`$ git reset --hard`"

#: ..\z_appendix.rst:35
msgid "Create patch (-M = detect renames –C = detect copies)"
msgstr "Patch erzeugen (--M = Umbenennungen erkennen –C = Kopien erkennen)"

#: ..\z_appendix.rst:35
msgid "`$ git format-patch –M –C origin`"
msgstr "`$ git format-patch –M –C origin`"

#: ..\z_appendix.rst:37
msgid "Apply patch without merging"
msgstr "Patch ohne zu mergen anwenden"

#: ..\z_appendix.rst:37
msgid "`$ git apply c:/patch/01-emp.patch`"
msgstr "`$ git apply c:/patch/01-emp.patch`"

#: ..\z_appendix.rst:39
msgid "Merge patch"
msgstr "Patch mergen"

#: ..\z_appendix.rst:39
msgid "`$ git am -–3way –-signoff c:/patch/01-emp.patch`"
msgstr "`$ git am -–3way –-signoff c:/patch/01-emp.patch`"

#: ..\z_appendix.rst:0 ..\z_appendix.rst:0
msgid "`$ git mergetool`"
msgstr "`$ git mergetool`"

#: ..\z_appendix.rst:0
msgid "`$ git am –-3way -–resolved`"
msgstr "`$ git am –-3way -–resolved`"

#: ..\z_appendix.rst:44
msgid "Stash changes"
msgstr "Änderungen stashen"

#: ..\z_appendix.rst:44
msgid "`$ git stash`"
msgstr "`$ git stash`"

#: ..\z_appendix.rst:46
msgid "Apply stashed changes"
msgstr "Änderungen aus Stash anwenden"

#: ..\z_appendix.rst:46
msgid "`$ git stash apply`"
msgstr "`$ git stash apply`"

#: ..\z_appendix.rst:48
msgid "Pull changes (add --rebase to rebase instead of merge)"
msgstr "Änderungen pullen (--rebase hinzufügen, um zu rebasen anstatt zu mergen)"

#: ..\z_appendix.rst:48
msgid "`$ git pull c:/demo1 master`"
msgstr "`$ git pull c:/demo1 master`"

#: ..\z_appendix.rst:0
msgid "`$ git commit`"
msgstr "`$ git commit`"

#: ..\z_appendix.rst:53
msgid "Push changes (in branch $ git push c:/demo1 master master:<new>)"
msgstr "Änderungen pushen (in Branch $ git push c:/demo1 master master:<neu>)"

#: ..\z_appendix.rst:53
msgid "`$ git push c:/demo1`"
msgstr "`$ git push c:/demo1`"

#: ..\z_appendix.rst:55
msgid "Blame"
msgstr "Blame"

#: ..\z_appendix.rst:55
msgid "`$ git blame –M –w <filename>`"
msgstr "`$ git blame –M –w <Dateiname>`"

#: ..\z_appendix.rst:57
msgid "Help"
msgstr "Hilfe"

#: ..\z_appendix.rst:57
msgid "`$ git <command> --help`"
msgstr "`$ git <Befehl> --help`"

#: ..\z_appendix.rst:60
msgid "Here are some default names used by Git."
msgstr "Hier sind einige Standardnamen die von Git verwendet werden."

#: ..\z_appendix.rst:63
msgid "Default names"
msgstr "Standardnamen"

#: ..\z_appendix.rst:65
msgid "master"
msgstr "master"

#: ..\z_appendix.rst:65
msgid "default branch"
msgstr "Standardbranch"

#: ..\z_appendix.rst:67
msgid "origin"
msgstr "origin"

#: ..\z_appendix.rst:67
msgid "default upstream repository"
msgstr "Standard Upstream Repository"

#: ..\z_appendix.rst:69
msgid "HEAD"
msgstr "HEAD"

#: ..\z_appendix.rst:69
msgid "current branch"
msgstr "aktueller Branch"

#: ..\z_appendix.rst:71
msgid "HEAD^"
msgstr "HEAD^"

#: ..\z_appendix.rst:71
msgid "parent of HEAD"
msgstr "Elternteil von HEAD"

#: ..\z_appendix.rst:73
msgid "HEAD~4"
msgstr "HEAD~4"

#: ..\z_appendix.rst:73
msgid "the great-great grandparent of HEAD"
msgstr "Der Groß-Groß-Großelternteil von HEAD"

#: ..\z_appendix.rst:78
msgid "Menu map"
msgstr "Übersicht Menü"

#: ..\z_appendix.rst:79
msgid ""
"The following image shows GitExtensions' menu structure at one glance "
"(v2.43):"
msgstr "Das folgende Bild zeigt die Menüstruktur von GitExtensions auf einen Blick (Stand Version 2.43)"
