#: ..\z_appendix.rst:2
msgid "Appendix"
msgstr "Appendice"

#: ..\z_appendix.rst:5
msgid "Git Cheat Sheet"
msgstr "Antisèche Git"

#: ..\z_appendix.rst:8
msgid "Action"
msgstr "Action"

#: ..\z_appendix.rst:8
msgid "Command"
msgstr "Commande"

#: ..\z_appendix.rst:10
msgid "Create new repository"
msgstr "Créer un nouveau dépôt"

#: ..\z_appendix.rst:10
msgid "`$ git init`"
msgstr "`$ git init`"

#: ..\z_appendix.rst:12
msgid "Create shared repository"
msgstr "Créer un dépôt partagé"

#: ..\z_appendix.rst:12
msgid "`$ git init –-bare –-shared=all`"
msgstr "`$ git init –-bare –-shared=all`"

#: ..\z_appendix.rst:14
msgid "Clone repository"
msgstr "Cloner un dépôt"

#: ..\z_appendix.rst:14
msgid "`$ git clone c:/demo1 c:/demo2`"
msgstr "`$ git clone c:/demo1 c:/demo2`"

#: ..\z_appendix.rst:16
msgid "Checkout branch"
msgstr "Récupérer le branche"

#: ..\z_appendix.rst:16
msgid "`$ git checkout <name>`"
msgstr "`$ git checkout <name>`"

#: ..\z_appendix.rst:18
msgid "Create branch"
msgstr "Créer une branche"

#: ..\z_appendix.rst:18
msgid "`$ git branch <name>`"
msgstr "`$ git branch <name>`"

#: ..\z_appendix.rst:20
msgid "Delete branch"
msgstr "Supprimer une branche"

#: ..\z_appendix.rst:20
msgid "`$ git branch -d <name>`"
msgstr "`$ git branch -d <name>`"

#: ..\z_appendix.rst:22
msgid "Merge branch (from the branch to merge into):"
msgstr " Fusionner une branche (depuis la branche dans laquelle fusionner) :"

#: ..\z_appendix.rst:22
msgid "`$ git merge PDC`"
msgstr "`$ git merge PDC`"

#: ..\z_appendix.rst:24 ..\z_appendix.rst:41 ..\z_appendix.rst:50
msgid "Solve conflicts (add --tool=kdiff3 if no mergetool is specified)"
msgstr "Résoudre les conflits (ajouter --tool=kdiff3 si aucun outil de fusion n'a été spécifié)"

#: ..\z_appendix.rst:24
msgid "`$ git mergetool` `$ git commit`"
msgstr "`$ git mergetool` `$ git commit`"

#: ..\z_appendix.rst:27
msgid "Create tag"
msgstr "Créer une étiquette"

#: ..\z_appendix.rst:27
msgid "`$ git tag <name>`"
msgstr "`$ git tag <name>`"

#: ..\z_appendix.rst:29
msgid "Add files/changes (. for all files)"
msgstr "Ajouter les fichiers/modifications (. pour tous les fichiers)"

#: ..\z_appendix.rst:29
msgid "`$ git add .`"
msgstr "`$ git add .`"

#: ..\z_appendix.rst:31
msgid "Commit added files/changes (--amend to amend to last commit)"
msgstr ""

#: ..\z_appendix.rst:31
msgid "`$ git commit –m “Enter commit message”`"
msgstr "`$ git commit –m “Entrer le message de commit”`"

#: ..\z_appendix.rst:33
msgid "Discard changes"
msgstr "Annuler les changements"

#: ..\z_appendix.rst:33
msgid "`$ git reset --hard`"
msgstr "`$ git reset --hard`"

#: ..\z_appendix.rst:35
msgid "Create patch (-M = detect renames –C = detect copies)"
msgstr "Créer un patch (-M  = détecter les fichiers renommés -C = détecter les fichiers copiés)"

#: ..\z_appendix.rst:35
msgid "`$ git format-patch –M –C origin`"
msgstr "`$ git format-patch –M –C origin`"

#: ..\z_appendix.rst:37
msgid "Apply patch without merging"
msgstr "Appliquer un patch sans fusionner"

#: ..\z_appendix.rst:37
msgid "`$ git apply c:/patch/01-emp.patch`"
msgstr "`$ git apply c:/patch/01-emp.patch`"

#: ..\z_appendix.rst:39
msgid "Merge patch"
msgstr "Fusionner un patch"

#: ..\z_appendix.rst:39
msgid "`$ git am -–3way –-signoff c:/patch/01-emp.patch`"
msgstr "`$ git am -–3way –-signoff c:/patch/01-emp.patch`"

#: ..\z_appendix.rst:0 ..\z_appendix.rst:0
msgid "`$ git mergetool`"
msgstr "`$ git mergetool`"

#: ..\z_appendix.rst:0
msgid "`$ git am –-3way -–resolved`"
msgstr "`$ git am –-3way -–resolved`"

#: ..\z_appendix.rst:44
msgid "Stash changes"
msgstr ""

#: ..\z_appendix.rst:44
msgid "`$ git stash`"
msgstr "`$ git stash`"

#: ..\z_appendix.rst:46
msgid "Apply stashed changes"
msgstr ""

#: ..\z_appendix.rst:46
msgid "`$ git stash apply`"
msgstr "`$ git stash apply`"

#: ..\z_appendix.rst:48
msgid "Pull changes (add --rebase to rebase instead of merge)"
msgstr ""

#: ..\z_appendix.rst:48
msgid "`$ git pull c:/demo1 master`"
msgstr "`$ git pull c:/demo1 master`"

#: ..\z_appendix.rst:0
msgid "`$ git commit`"
msgstr "`$ git commit`"

#: ..\z_appendix.rst:53
msgid "Push changes (in branch $ git push c:/demo1 master master:<new>)"
msgstr ""

#: ..\z_appendix.rst:53
msgid "`$ git push c:/demo1`"
msgstr "`$ git push c:/demo1`"

#: ..\z_appendix.rst:55
msgid "Blame"
msgstr "Blâmer"

#: ..\z_appendix.rst:55
msgid "`$ git blame –M –w <filename>`"
msgstr "`$ git blame –M –w <filename>`"

#: ..\z_appendix.rst:57
msgid "Help"
msgstr "Aide"

#: ..\z_appendix.rst:57
msgid "`$ git <command> --help`"
msgstr "`$ git <command> --help`"

#: ..\z_appendix.rst:60
msgid "Here are some default names used by Git."
msgstr ""

#: ..\z_appendix.rst:63
msgid "Default names"
msgstr "Noms par défaut"

#: ..\z_appendix.rst:65
msgid "master"
msgstr "master"

#: ..\z_appendix.rst:65
msgid "default branch"
msgstr "branche par défaut"

#: ..\z_appendix.rst:67
msgid "origin"
msgstr "origin"

#: ..\z_appendix.rst:67
msgid "default upstream repository"
msgstr ""

#: ..\z_appendix.rst:69
msgid "HEAD"
msgstr "HEAD"

#: ..\z_appendix.rst:69
msgid "current branch"
msgstr "branche actuelle"

#: ..\z_appendix.rst:71
msgid "HEAD^"
msgstr "HEAD^"

#: ..\z_appendix.rst:71
msgid "parent of HEAD"
msgstr ""

#: ..\z_appendix.rst:73
msgid "HEAD~4"
msgstr "HEAD~4"

#: ..\z_appendix.rst:73
msgid "the great-great grandparent of HEAD"
msgstr ""

